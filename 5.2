#include <iostream>
#include <vector>

using namespace std;

int main() {
    int n;
    cout << "Podaj liczbe elementow tablicy: ";
    cin >> n;

    vector<int> array(n);

    cout << "Podaj elementy tablicy: ";
    for (int i = 0; i < n; ++i) {
        cin >> array[i];
    }

    cout << "Tablica przed zamiana: ";
    for (int i = 0; i < n; ++i) {
        cout << array[i] << " ";
    }
    cout << endl;

    for (int i = 0; i < n / 2; ++i) {
        swap(array[i], array[n - i - 1]);
    }

    cout << "Tablica po zamianie: ";
    for (int i = 0; i < n; ++i) {
        cout << array[i] << " ";
    }
    cout << endl;

    return 0;
}
